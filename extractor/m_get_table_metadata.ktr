      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);

    if ( process ){
		String json = get(Fields.In, "json").getString(r);
		Boolean last = get(Fields.In, "last").getBoolean(r);
		String column_name = get(Fields.In, "column_name").getString(r);
		String column_name_lower = get(Fields.In, "column_name_lower").getString(r);
		String reserved_words = get(Fields.In, "reserved_words").getString(r);
	
		//Normalize column names and replace reserved words. 
		json = json.replaceAll( '"' + column_name + '"', '"' + column_name_lower + '"' ); 

        //Identifies the first row.
        if (first) {
            first = false;
			
			//Open the json fields array. 
			get(Fields.In, "json").setValue(r, "[" + json + "," );
        }else if( last ) {
			//Close the json fiels array. 
			get(Fields.In, "json").setValue(r, json + "]" );
		}else{
			get(Fields.In, "json").setValue(r, json + "," );
		}

        //Creates the output row. 
        r = createOutputRow(r, data.outputRowMeta.size());

        //Send the row to the output port. 
        putRow(data.outputRowMeta, r);
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
      </definition>
