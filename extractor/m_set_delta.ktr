    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.regex.Matcher;
import java.text.SimpleDateFormat;
import java.util.Date;
import java.util.Calendar;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow();
    boolean process = (r != null);
	boolean update = true;  

    if( process ){
		Object[] out = createOutputRow(r, data.outputRowMeta.size());
		String deltaInFile = get(Fields.In, "delta_value").getString(r) == null ? "" : get(Fields.In, "delta_value").getString(r);
		String deltaInQueue = get(Fields.In, "DELTA_FIELD_IN_QUEUE").getString(r) == null ? "" : get(Fields.In, "DELTA_FIELD_IN_QUEUE").getString(r);
		Long delay = get(Fields.In, "DELTA_DELAY").getInteger(r);

		//Identifies if the delta is a integer. 
		if( deltaInQueue.matches("\\d+") ){
			if ( deltaInFile.matches("\\d+") ){
				//Compares delta integer values. 
				update = Long.valueOf( deltaInQueue ) > Long.valueOf( deltaInFile );
			}	
		//Identifies if the delta is a date. 
		}else if ( deltaInQueue.matches("\\d{4}-\\d{2}-\\d{2}") ){
			if ( deltaInFile.matches("\\d{4}-\\d{2}-\\d{2}") ){
				try{
					//Defines the date format. 
					SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
					
					//Converts the delta value to date. 
					Date dateInQueue = sdf.parse( deltaInQueue );
					Date dateInFile = sdf.parse( deltaInFile );

					//Compares delta date values. 
					update = ( dateInQueue.compareTo( dateInFile ) == 1 );
				}catch(Exception ex){
					logBasic( "Invalid delta value!"); 
				}
			}
		//Identifies if the delta is a timestamp. 
		}else if ( deltaInQueue.matches("\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}") ){
			if ( deltaInFile.matches("\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}") ){
				try{		
					//Defines the timestamp format. 
					SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");
					
					//Converts the delta value to timestamp. 
					Date dateInQueue = sdf.parse( deltaInQueue );
					Date dateInFile = sdf.parse( deltaInFile );
					
					//Compares delta timestamp values. 
					update = ( dateInQueue.compareTo( dateInFile ) == 1 );

					if ( update ){
						//Identify if there was setted a delta delay.
						Calendar calendar = Calendar.getInstance();
						calendar.setTime(dateInQueue);
						calendar.add(Calendar.MINUTE, (int) ( delay * -1 ) );

						logBasic( "Delta value was decreased in " + delay + " minutes to " + sdf.format(calendar.getTime()) + "!");
						get(Fields.Out, "DELTA_FIELD_IN_QUEUE").setValue(out, sdf.format(calendar.getTime()) );
					}				
				}catch(Exception ex){
					logBasic( "Invalid delta value!"); 
				}
			}
		} else if ( deltaInQueue.isEmpty() ) {
			update = false; 
		}

		//Set the row value. 
		get(Fields.Out, "update").setValue( out, update );
        putRow(data.outputRowMeta, out);
    }else{
      setOutputDone();
    }

    return process;
}</class_source>
      </definition>
